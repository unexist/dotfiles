# urxvt
*background: #202020
*foreground: #e2e2e5
*scrollBar: false
*loginShell: true
*font: -*-lucidatypewriter-medium-*-*-*-12-*-*-*-*-*-*-*
*termName: rxvt-256color

# Black + DarkGrey
*color0: #000000
*color8: #3c414c

# DarkRed + Red
*color1: #963C59
*color9: #CF6171

# DarkGreen + Green
*color2: #76B243
*color10: #A8FF60

# DarkYellow + Yellow
*color3: #B2B27F
*color11: #FFFFB6

# DarkBlue + Blue
*color4: #5986B2
*color12: #6699CC

# DarkMagenta + Magenta
*color5: #B250B1
*color13: #FF73FD

# DarkCyan + Cyan
*color6: #698FB2
*color14: #96CBFE

# LightGrey + White
*color7: #8f8f8f
*color15: #F6F3E8

# Urls
*keysym.M-u: perl:mark-yank-urls:activate_mark_mode
*underlineURLs: true
*perl-lib: /home/unexist/.urxvt/
*perl-ext: selection,mark-yank-urls
*urlLauncher: uzbl --uri 
