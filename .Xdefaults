# urxvt
*background: #202020
*foreground: #e2e2e5
*scrollBar: false
*loginShell: true
*font: -*-lucidatypewriter-medium-*-*-*-12-*-*-*-*-*-*-*
*termName: rxvt-256color

# Black + DarkGrey
*color0: #000000
*color8: #3c414c

# DarkRed + Red
*color1: #963C59
*color9: #CF6171

# DarkGreen + Green
*color2: #73862F
*color10: #b1d631

# DarkYellow + Yellow
*color3: #fff796
*color11: #faf4c6

# DarkBlue + Blue
*color4: #515c71
*color12: #7e8aa2

# DarkMagenta + Magenta
*color5: #cf7c00
*color13: #ff9800

# DarkCyan + Cyan
*color6: #b4eeb4
*color14: #71bebe

# LightGrey + White
*color7: #8f8f8f
*color15: #F6F3E8

# Urls
*keysym.M-u: perl:mark-yank-urls:activate_mark_mode
*underlineURLs: true
*perl-lib: /home/unexist/.urxvt/
*perl-ext: selection,mark-yank-urls
*urlLauncher: uzbl --uri 
