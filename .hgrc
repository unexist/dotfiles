[ui]
editor = vim
username = unexist
commitsubrepos = false

[extensions]
hgext.keyword     =
hgext.record      =
hgext.graphlog    =
hgext.color       =
hgext.transplant  =
hgext.histedit    =
hgext.strip       =
mercurial_keyring =
#mercurial_keyring = $HOME/Library/Python/3.9/lib/python/site-packages/mercurial_keyring.py

[extdiff]
cmd.dft = difft
opts.dft = --missing-as-empty

[keyword]
**.* =

[keywordmaps]
RCSFile = {file|basename}
Author = {author|user}
Header = {file},v {rev} {date|utcdate} {author|user}
Source = {file}
Date = {date|utcdate}
Id = {file},v {rev} {date|utcdate} {author|user}
Rev = {rev}

[color]
mode = auto

[auth]
unexistdev.prefix   = https://hg.unexist.dev
unexistdev.username = unexist
