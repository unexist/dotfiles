[ui]
editor = vim
username = unexist
commitsubrepos = false

[extensions]
hgext.keyword    =
hgext.record     =
hgext.graphlog   =
hgext.color      =
hgext.transplant =
hgcrecord        = /home/unexist/.crecord/crecord
histedit         = /home/unexist/.histedit/hg_histedit.py

[keyword]
**.* =

[keywordmaps]
RCSFile = {file|basename}
Author = {author|user}
Header = {file},v {rev} {date|utcdate} {author|user}
Source = {file}
Date = {date|utcdate}
Id = {file},v {rev} {date|utcdate} {author|user}
Rev = {rev}

[color]
mode = auto
