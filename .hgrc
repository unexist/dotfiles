[ui]
editor = vim
username = unexist

[extensions]
hgext.keyword  =
hgext.record   =
hgext.graphlog =
hgext.color    =

[keyword]
**.* =

[keywordmaps]
RCSFile = {file|basename}
Author = {author|user}
Header = {file},v {rev} {date|utcdate} {author|user}
Source = {file}
Date = {date|utcdate}
Id = {file},v {rev} {date|utcdate} {author|user}
Rev = {rev}

[color]
mode = auto
